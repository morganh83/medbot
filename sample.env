DISCORD_TOKEN="TOKEN"

JOIN_INTAKE_CHANNEL_ID=123456789
BOOKINGS_CHANNEL_ID=123456789
DISPATCH_CHANNEL_ID=123456789
DISPATCH_PING_ROLE_ID=123456789

# Web server config for receiving form submissions.
WEB_BIND_HOST="0.0.0.0"
WEB_BIND_PORT="8080"

# Shared secret to prevent random people from POSTing to your webhook.
# Your website should send this in header: X-MedBot-Secret: <value>
WEBHOOK_SHARED_SECRET="CHANGE_ME"

# Database path for incident tracking
DB_PATH="medbot.db"

# Timezone choice. Discord stores timestamps in UTC; this is only for the incident ID formatting.
# For America/New_York without external libs, we keep it simple and use local time as "naive".
# If you want true timezone handling, we can switch to zoneinfo.
LOCAL_TIME_LABEL="America/New_York"

# Optional: When true, also write CSV snapshots for easy spreadsheet import.
EXPORT_CSV=true
CSV_EXPORT_PATH="incidents_export.csv"